      page:1,
      loading: false
    };
  },
  methods: {
      load () {
        this.loading = true
        setTimeout(() => {
          this.$http.get("/category/book/id?id="+this.$route.params.id+"&page="+(this.page+1)).then(res => console.info(this.books = res.data))
          this.loading = false
        }, 2000)
      }
  },
  computed: {
      noMore () {
        return this.count >= this.total
      },
      disabled () {
        return this.loading || this.noMore
      }
  },
  async created() {
    await this.$http.get("/category/id?id="+this.$route.params.id).then(res => this.collectionsBooks = res.data.data)
